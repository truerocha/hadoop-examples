# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index


# Landing Page
#GET     /land                       controllers.Application.landing

# Dashboard Page
GET     /dashboard                  controllers.Application.dashboard

# Dashboard Source System Page
#GET     /dash_ss                  	controllers.Application.dashSourceSystem

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

# Get Tables Page
GET     /getTables                  controllers.Application.getTables

# Get Source Lineage Page
GET     /getSourceLineage           controllers.Application.getSourceLineage

# Get Columns Page
GET     /getColumns                	controllers.Application.getColumns


# get Source Matrix Page
GET     /getSourceMatrix               	controllers.Application.getSourceMatrix
